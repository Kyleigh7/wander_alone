/**
 *Class for the dark big trees
 */
class BigTreesDark
{
  private float x, y;//for the position of the trees
  private int w, l; //for image size
  
  //constanat
  private final PImage bigTreeDarkImg = loadImage("Tree_Big_Dark.png");
  
  //constructor
  public BigTreesDark(float x, float y, int w, int l)
  {
    this.x = x;
    this.y = y;
    this.w = w;
    this.l = l; 
  }
  
  /**
   *displys the trees
   */
  public void display()
  {
    image(bigTreeDarkImg, x, y, w, l);
  }
  
  /**
   *moves the trees
   */
  public void moveRight()
  {
    x -= 37;
  }
}
